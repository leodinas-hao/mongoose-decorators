{
  "name": "mongoose-schema-decorators",
  "version": "1.1.4",
  "description": "Define your mongoose models/schemas easily with @decorators",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "compile": "npm run clean && tsc",
    "prepublishOnly": "npm run compile",
    "pretest": "npm run compile",
    "test": "mocha ./lib/**/*.spec.js",
    "test:debug": "mocha --inspect=5858 --colors --no-timeouts --require ts-node/register ./src/**/*.spec.ts",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leodinas-hao/mongoose-schema-decorators.git"
  },
  "keywords": [
    "nodejs",
    "mongoose",
    "typescript",
    "decorators"
  ],
  "author": "Leodinas Hao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leodinas-hao/mongoose-schema-decorators/issues"
  },
  "homepage": "https://github.com/leodinas-hao/mongoose-schema-decorators#readme",
  "dependencies": {
    "mongoose": "^5.7.7",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.23",
    "@types/node": "^12.11.7",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "mocha-typescript": "^1.1.17",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  }
}
